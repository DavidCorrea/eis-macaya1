		<table border="1" style="width:500px">
		<h3>Creacion de Partido</h3>
			<% form_for(@partido, '/partidos/create') do |p| %> 
                                <%= p.label :torneo %>
				<%= p.text_field :torneo %> <br>
                                <%= p.label :fecha %>	
				<%= p.text_field :fecha %> <br>
				<%= p.label :equipo_local %>
				<%= p.text_field :equipo_local %> <br>
				<%= p.label :equipo_visitante %>
				<%= p.text_field :equipo_visitante %> <br>
							
				<%= p.submit "Agregar", :class => 'button' %>
			<% end %>
		</table>
</div>

